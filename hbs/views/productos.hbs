{{#if showList }}
<h2 class="text-danger mt-4 fs-2">Productos cargados</h2>
<table class="table table-striped">
        <thead class="fs-4">
          <tr>
            <th scope="col">#</th>
            <th scope="col">TÃ­tulo</th>
            <th scope="col">Precio</th>
            <th scope="col">URL imagen</th>
          </tr>
        </thead>
        <tbody class="fs-5">
          {{#each list}}
              <tr>
                  <th scope="row">{{this.id}}</th>
                  <td>{{this.title}}</td>
                  <td>{{this.price}}</td>
                  <td><img src="{{this.thumbnail}}" alt="" width="40px" height="40px"></td>
                </tr>
          {{/each}}
        </tbody>
      </table>
  {{else}}
    <h5 class="alert-danger rounded p-2">No se encontraron datos</h5>  
{{/if}}
